= render :partial => 'shared/public'

= content_for :custom do

  = render :partial => 'shared/top'
  
  - form_for :member, :url => members_path, :html => { :multipart => 'true' } do |f|
    = render :partial => "form", :locals => {:f => f, :submit_label => "Submit"}
    %p.buttons
      = f.submit 'Submit'
      or
      = link_to 'Cancel', members_path
